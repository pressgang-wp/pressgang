<!doctype html>
<html {{ site.language_attributes }}>
{% block head %}
	{% include 'scaffold/head.twig' %}
{% endblock %}
<body class="{{ body_class }}">
{% do action('wp_body_open') %}
{% block body %}
	<div id="wrap">
		<noscript>
			<strong>{{ __("Warning!", THEMENAME) }}</strong> {{ __("Your browser does not support JavaScript!", THEMENAME) }}
		</noscript>
		{% block header %}
			<header id="header">
				{% include 'shared/header.twig' %}
			</header>
		{% endblock %}
		{% block main %}
			<main id="main">
				{% block content %}
					{{ __("Sorry, there is no content to display!", THEMENAME) }}
				{% endblock %}
			</main>
		{% endblock %}
	</div>
	{% block footer %}
		<footer id="footer">
			{% include 'shared/footer.twig' %}
		</footer>
	{% endblock %}
{% endblock %}
{{ fn('wp_footer') }}
</body>
</html>
