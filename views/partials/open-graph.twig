<!-- open graph -->
<meta property="og:site_name" content="{{ open_graph.site_name }}"/>
<meta property="og:title" content="{{ open_graph.title }}"/>
<meta property="og:description" content="{{ open_graph.description|e('html_attr') }}"/>
<meta property="og:type" content="{{ open_graph.type|e('html_attr') }}"/>
<meta property="og:url" content="{{ open_graph.url|e('esc_url') }}"/>
<meta property="og:image" content="{{ open_graph.image|e('esc_url') }}"/>
